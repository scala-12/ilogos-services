{
  "name": "auth-service-v2",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "win:gen:proto": "rimraf ./generated && mkdirp ./generated && protoc --plugin=protoc-gen-ts_proto=\".\\node_modules\\.bin\\protoc-gen-ts_proto.cmd\" --ts_proto_out=generated --ts_proto_opt=outputServices=grpc-js -I ../shared/proto ../shared/proto/*.proto",
    "build": "tsc -p tsconfig.json",
    "start": "node build/app.js",
    "test": "node --test test/**/*.test.js",
    "debug": "node --inspect -r tsx app.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/postgres": "^6.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.0",
    "@grpc/grpc-js": "^1.13.3",
    "@sinclair/typebox": "^0.34.33",
    "bcrypt": "^6.0.0",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "shared-lib": "file:../shared/kmp-library/build/js/packages/shared-lib"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.19",
    "mkdirp": "^3.0.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.7.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
